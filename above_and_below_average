def my_function(my_list):
    sum_of_all = 0
    count_of_all = 0
    for i in my_list:
        sum_of_all += i
        count_of_all += 1
    average_sum = sum_of_all / count_of_all
    for j in my_list:
        if j < average_sum:
            min_list.append(j)
        elif j > average_sum:
            max_list.append(j)
    print(min_list)
    print(max_list)


my_list = []
for k in range(10):
    num = int(input())
    my_list.append(num)
min_list = []
max_list = []
print(my_list)
my_function(my_list)
